{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --coverage.threshold.lines=60 --coverage.threshold.functions=60 --coverage.threshold.branches=60 --coverage.threshold.statements=60",
    "test:a11y": "vitest run --testPathPattern=accessibility",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "a11y:audit": "node scripts/accessibility-audit.js"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.4",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.19.28",
    "@types/react": "^18.3.27",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "^23.0.1",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.6.15",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.3.3",
    "vite": "^7.2.4",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^4.42.0",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.7.4",
    "lucide-react": "^0.562.0",
    "msw": "^2.12.7",
    "react-i18next": "^16.5.2",
    "react-router-dom": "^7.12.0",
    "tailwind-merge": "^3.4.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
